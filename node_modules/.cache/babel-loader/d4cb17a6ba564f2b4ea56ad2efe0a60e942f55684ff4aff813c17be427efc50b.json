и:', error);\n  //     });\n  // },\n\n  mounted() {\n    this.$store.dispatch('fetchMeetingsList');\n  }\n};\n</script>\n\n<style scoped>\nh2 {\n  margin-bottom: 20px;\n  text-align: center;\n}\n\n.status-init {\n  color: #ff6f00;\n  /* Ярко-оранжевый */\n  font-weight: bold;\n  padding: 5px;\n}\n\n.status-error {\n  color: #d32f2f;\n  /* Ярко-красный */\n  font-weight: bold;\n  padding: 5px;\n}\n\n.status-upload_docs {\n  color: #0288d1;\n  /* Ярко-голубой */\n  font-weight: bold;\n  padding: 5px;\n}\n\n.status-process {\n  color: #388e3c;\n  /* Ярко-зеленый */\n  font-weight: bold;\n  padding: 5px;\n}\n\n.status-reupload_fls {\n  color: #7b1fa2;\n  /* Ярко-фиолетовый */\n  font-weight: bold;\n  padding: 5px;\n}\n</style>"],"mappings":"AAoBA,SAAAA,QAAA;AACA,OAAAC,MAAA;AAGA;EACAC,QAAA;IACA,GAAAF,QAAA;IACAG,QAAA;MACA,QACA;QAAAC,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,EACA;IACA;EACA;EACAC,OAAA;IACAC,WAAAC,IAAA;MACA,OAAAP,MAAA,CAAAO,IAAA,EAAAC,MAAA;IACA;IACAC,YAAAC,MAAA;MACA;QACA,eAAAA,MAAA;QACA,gBAAAA,MAAA;QACA,sBAAAA,MAAA;QACA,kBAAAA,MAAA;QACA,uBAAAA,MAAA;MACA;IACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;EACA;;EAEAC,QAAA;IACA,KAAAC,MAAA,CAAAC,QAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}