{"ast":null,"code":"import moment from 'moment';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      order: null\n    };\n  },\n  mounted() {\n    const id = this.$route.params.id;\n    this.fetchOrderDetails(id);\n  },\n  methods: {\n    fetchOrderDetails(id) {\n      axios.get(`https://my-json-server.typicode.com/plushevy/demo/orders/${id}`).then(response => {\n        this.order = response.data;\n      }).catch(error => {\n        console.error('Ошибка при загрузке данных заявки:', error);\n      });\n    },\n    formatDate(date) {\n      return moment(date).isValid() ? moment(date).format('DD.MM.YYYY HH:mm') : 'Invalid date';\n    }\n  }\n};","map":{"version":3,"names":["moment","axios","data","order","mounted","id","$route","params","fetchOrderDetails","methods","get","then","response","catch","error","console","formatDate","date","isValid","format"],"sources":["src/components/OrderDetails.vue"],"sourcesContent":["<template>\n  <v-card>\n    <h2>Детали заявки</h2>\n    <div v-if=\"order\">\n      <p><strong>ID:</strong> {{ order.id }}</p>\n      <p><strong>Номер:</strong> {{ order.num }}</p>\n      <p><strong>Дата добавления:</strong> {{ formatDate(order.dadd) }}</p>\n      <p><strong>Профиль ID:</strong> {{ order.profile_id }}</p>\n      <p><strong>Статус:</strong> {{ order.state }}</p>\n      <p><strong>Последнее обновление:</strong> {{ formatDate(order.lastUpd) }}</p>\n      <p><strong>Продукт:</strong> {{ order.stg.join(', ') }}</p>\n      <p><strong>ИНН:</strong> {{ order.inn }}</p>\n      <p><strong>Телефон:</strong> {{ order.person_phone }}</p>\n      <p><strong>Тип компании:</strong> {{ order.company_type }}</p>\n      <p><strong>Email:</strong> {{ order.person_email }}</p>\n      <p><strong>Источник CRM:</strong> {{ order.extra.crm_source_type }}</p>\n      <div v-if=\"order.extra?.partner\">\n        <h3>Партнер</h3>\n        <p><strong>Тип:</strong> {{ order.extra.partner.type }}</p>\n        <p><strong>User ID:</strong> {{ order.extra.partner.user_id }}</p>\n        <p><strong>Имя клиента:</strong> {{ order.extra.partner.client_name }}</p>\n        <p><strong>ИНН:</strong> {{ order.extra.partner.inn }}</p>\n      </div>\n    </div>\n    <div v-else>\n      <p>Загрузка данных...</p>\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport moment from 'moment';\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      order: null\n    };\n  },\n  mounted() {\n    const id = this.$route.params.id; \n    this.fetchOrderDetails(id);\n  },\n  methods: {\n    fetchOrderDetails(id) {\n      axios\n        .get(`https://my-json-server.typicode.com/plushevy/demo/orders/${id}`)\n        .then(response => {\n          this.order = response.data;\n        })\n        .catch(error => {\n          console.error('Ошибка при загрузке данных заявки:', error);\n        });\n    },\n    formatDate(date) {\n      return moment(date).isValid() ? moment(date).format('DD.MM.YYYY HH:mm') : 'Invalid date';\n    }\n  }\n};\n</script>\n\n<style scoped>\nh2 {\n  margin-bottom: 20px;\n  text-align: center;\n}\n</style>"],"mappings":"AA+BA,OAAAA,MAAA;AACA,OAAAC,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,MAAAC,EAAA,QAAAC,MAAA,CAAAC,MAAA,CAAAF,EAAA;IACA,KAAAG,iBAAA,CAAAH,EAAA;EACA;EACAI,OAAA;IACAD,kBAAAH,EAAA;MACAJ,KAAA,CACAS,GAAA,6DAAAL,EAAA,IACAM,IAAA,CAAAC,QAAA;QACA,KAAAT,KAAA,GAAAS,QAAA,CAAAV,IAAA;MACA,GACAW,KAAA,CAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,uCAAAA,KAAA;MACA;IACA;IACAE,WAAAC,IAAA;MACA,OAAAjB,MAAA,CAAAiB,IAAA,EAAAC,OAAA,KAAAlB,MAAA,CAAAiB,IAAA,EAAAE,MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}